"this program will take _FunnyValue, then add 2, then subtract 1, then repeat
put _FunnyValue = 0;
lab MainLoop;
print _FunnyValue;
sub Add2;
sub Sub1;
jump MainLoop;

"helper routine to add 2
lab Add2;
put _FunnyValue = _FunnyValue + 2;
return;

"helper routine to subtract 1
lab Sub1;
put _FunnyValue = _FunnyValue - 1;
return;
